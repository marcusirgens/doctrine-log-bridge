{
    "name": "marcuspi/doctrine-log-bridge",
    "description": "Allows you to pass a PSR-3 compliant logger to Doctrine",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcus Pettersen Irgens",
            "email": "marcus.pettersen.irgens@gmail.com"
        }
    ],
    "keywords": [
        "doctrine",
        "psr-3",
        "logger",
        "dbal",
        "sqllogger"
    ],
    "require": {
        "psr/log": "^1",
        "php": "^7.1",
        "doctrine/dbal": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Marcuspi\\DoctrineLogBridge\\": "src/DoctrineLogBridge/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marcuspi\\DoctrineLogBridge\\Test": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.4",
        "php": "^7.3"
    }
}
